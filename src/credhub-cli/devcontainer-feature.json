{
    "name": "CredHub CLI",
    "id": "credhub-cli",
    "version": "1.0.1",
    "description": "Installs the CredHub CLI.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "2",
                "none"
            ],
            "default": "latest",
            "description": "Select or enter a CredHub CLI version."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
